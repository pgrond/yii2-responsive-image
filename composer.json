{
  "name": "pgrond/yii2-responsive-image",
  "description": "Create thumbnails with custom presets and use them in responsive widgets",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "thumbnail",
    "responsive",
    "image"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Lutzenberger",
      "email": "lutzenbergerthomas@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.38",
    "yiisoft/yii2-imagine": "~2.2.0",
    "symfony/console": ">=2.7 <6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "TomLutzenberger\\ResponsiveImage\\": "src"
    }
  }
}
